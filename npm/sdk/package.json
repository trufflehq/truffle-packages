{
  "name": "@trufflehq/sdk",
  "version": "0.5.0-alpha.9",
  "description": "The official javascript SDK for Truffle",
  "keywords": [
    "truffle",
    "truffle.tv",
    "youtube",
    "twitch"
  ],
  "author": "Spore Labs, Inc.",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "tsup",
    "clean": "rimraf dist",
    "build": "npm run clean && tsup",
    "watch": "tsup --watch",
    "test": "jest",
    "format": "eslint --ext .ts,.tsx,.js,.jsx src examples --fix --debug"
  },
  "dependencies": {
    "@trufflehq/mothertree-client": "^0.1.0-alpha.13",
    "@trufflehq/transframe": "^0.1.0",
    "@urql/core": "^3.1.1",
    "@urql/exchange-auth": "^1.0.0",
    "graphql": "^16.8.0",
    "graphql-ws": "^5.13.1",
    "wonka": "^6.1.2"
  },
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.3.1",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "rxjs": "^7.8.0",
    "ts-jest": "^29.0.5",
    "tsup": "^6.6.3",
    "typescript": "^4.9.4"
  },
  "volta": {
    "node": "18.15.0"
  }
}
